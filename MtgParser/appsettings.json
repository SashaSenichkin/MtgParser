{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information"
            }
        },
        "WriteTo": [
            {
                "Name": "File",
                "Args": { "path":  "./logs/log-.txt", "rollingInterval": "Day" }
            }
        ]
    },
    "ExternalUrls": {
        "BaseMtgRu": "http://www.mtg.ru/cards/search.phtml?Title=",
        "MtgRuInfoTable": "http://www.mtg.ru/cards/card_info_table.phtml?Action=ShowCardVersion&card=",
        "PriceApi": "https://www.mtggoldfish.com/price/"
    }
}